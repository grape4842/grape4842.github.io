  <script type="text/javascript">
	  var today=new Date();
	var year=today.getYear();
	var month=today.getMonth();
	var date=today.getDate();
	var day=today.getDay();

	switch(day) {
	case 0:day="일"
	

	case 1:day="월"
	
	
	case 2:day="화"
	
	
	case 3:day="수"
	
	
	case 4:day="목"
	
	
	case 5:day="금"
	
	
	case 6:day="토"
	}
	alert("오늘은"+(year+1900)+"년 "+(1+month+"월 ")+date+"일 "+day+"요일"+"입니다.");
	
	var timeh= new Date();
	var hours=timeh.getHours();
	switch (hours)
	{
		case 7:alert ("아침시간입니다."); break;
		case 8:alert ("아침시간입니다."); break;
		case 9:alert ("오전수업시간입니다."); break;
		case 10:alert ("오전수업시간입니다."); break;
		case 11:alert ("오전수업시간입니다."); break;
		case 12:alert ("점심시간입니다."); break;
		case 13:alert ("오후수업시간입니다."); break;
		case 14:alert ("오후수업시간입니다."); break;
		case 15:alert ("오후수업시간입니다."); break;
		case 16:alert ("오후수업시간입니다."); break;
		default: alert ("자유시간입니다."); break;
	}
  </script>
  
